{
	"name": "Python 3",
	"image": "mcr.microsoft.com/devcontainers/python:3.10-bullseye",
	"hostRequirements": {
		"cpus": 4
	},
	"features": {
		"ghcr.io/devcontainers/features/node:1": {
			"version": "19"
		},
		"ghcr.io/devcontainers/features/docker-from-docker:1": {}
	},
	"onCreateCommand": "./develop.sh",
	"customizations": {
		"codespaces": {
			"repositories": {
				"my_org/*": {
					"permissions": {
						"permissions": "write-all"
					}
				}
			}
		},
		"vscode": {
			"extensions": [
				"ms-toolsai.jupyter",
				"ms-python.python"
			]
		}
	}
}